<div class="row">
  <div class="span6 offset3">
    <%= form_for(@student) do |f| %>

        <%= f.label :first_name %>
        <%= f.text_field :first_name %>

        <%= f.label :last_name %>
        <%= f.text_field :last_name %>

        <%= f.submit "Save", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>